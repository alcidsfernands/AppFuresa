<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AppFuresa.Flyouts.FlyoutPrincipal"
             xmlns:local="clr-namespace:AppFuresa.Flyouts"
                xmlns:pagesDetail="clr-namespace:AppFuresa.TabPaginas">
    <FlyoutPage.Flyout BackgroundColor="Black">
        <local:FlyoutPrincipalFlyout x:Name="flyoutPage"/>
    </FlyoutPage.Flyout>
    <FlyoutPage.Detail>
    <NavigationPage>
      <x:Arguments>
        <pagesDetail:TabPrincipal/>
      </x:Arguments>
    </NavigationPage>
  </FlyoutPage.Detail>
</FlyoutPage>